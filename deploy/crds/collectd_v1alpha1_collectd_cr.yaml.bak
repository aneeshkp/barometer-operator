global:
    Hostname: "localhost"
    FQDNLookup: true
    BaseDir:  "@localstatedir@/lib/@PACKAGE_NAME@"
    PIDFile: "@localstatedir@/run/@PACKAGE_NAME@.pid"
    PluginDir: "@libdir@/@PACKAGE_NAME@"
    TypesDB: "@prefix@/share/@PACKAGE_NAME@/types.db"
    AutoLoadPlugin:  false
  plugins:
    cpu: 
      Include: 
        attributes:
         - "/etc/collectd.d/*.conf"

        Filter: ssdsd
    aggregation:
      Aggregation: 
        Plugin: cpu
        Type: cpu
        GroupBy:
          - Host
          - TypeInstance
        CalculateAverage: true
    df:
      Device: /dev/hda1
      MountPoint: /home
      FSType: ext3
      IgnoreSelected: false
      ReportInodes: false
    match_regex:
     Chain: 
       name: PostCache 
     Rule
      Match 
        Name: regex
        Plugin: ^cpu$
        PluginInstance: ^[0-9]+$
    
    <Target write>
      Plugin "aggregation"
    </Target>
    Target stop
  </Rule>
  <Target write> # Write everything else via "rrdtool".
    Plugin "rrdtool"
  </Target>
</Chain>